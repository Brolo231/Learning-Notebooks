


class Solution:
    def containsAnagram(self, sentence: str) -> bool:
        seen = []
    
        for word in sentence.split(" "):
            if sorted(word.strip().lower()) in seen:
                return True
            else:
                seen.append(sorted(word.strip().lower()))

        return False

s = Solution()
s.containsAnagram("Don't ignore that hat or that region")
             
